{
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "Referrer-Policy",
          "value" : "origin-when-cross-origin"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "SAMEORIGIN"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=2592000"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/home-v1/faq", "destination": "/index.html" },
    { "source": "/home-v1/brand", "destination": "/index.html" },
    { "source": "/home-v1/ambassador", "destination": "/index.html" },
    { "source": "/home-v1/community", "destination": "/index.html" },
    { "source": "/home-v1/blog", "destination": "/index.html" },
    { "source": "/home-v1/blog/(.*)", "destination": "/index.html" },
    { "source": "/home-v1/reports", "destination": "/index.html" },
    { "source": "/home-v1/events", "destination": "/index.html" },
    { "source": "/home-v1/videos", "destination": "/index.html" },
    { "source": "/home-v1/news", "destination": "/index.html" },
    { "source": "/home-v1/tech", "destination": "/index.html" },
    { "source": "/home-v1/economic_model", "destination": "/index.html" },
    { "source": "/home-v1/plo", "destination": "/index.html" },
    { "source": "/home-v1/plo_contribute", "destination": "/index.html" }
  ]
}
