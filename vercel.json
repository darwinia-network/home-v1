{
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "Referrer-Policy",
          "value" : "origin-when-cross-origin"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "SAMEORIGIN"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=2592000"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/legacy-home/faq", "destination": "/index.html" },
    { "source": "/legacy-home/brand", "destination": "/index.html" },
    { "source": "/legacy-home/ambassador", "destination": "/index.html" },
    { "source": "/legacy-home/community", "destination": "/index.html" },
    { "source": "/legacy-home/blog", "destination": "/index.html" },
    { "source": "/legacy-home/blog/(.*)", "destination": "/index.html" },
    { "source": "/legacy-home/reports", "destination": "/index.html" },
    { "source": "/legacy-home/events", "destination": "/index.html" },
    { "source": "/legacy-home/videos", "destination": "/index.html" },
    { "source": "/legacy-home/news", "destination": "/index.html" },
    { "source": "/legacy-home/tech", "destination": "/index.html" },
    { "source": "/legacy-home/economic_model", "destination": "/index.html" },
    { "source": "/legacy-home/plo", "destination": "/index.html" },
    { "source": "/legacy-home/plo_contribute", "destination": "/index.html" }
  ]
}
